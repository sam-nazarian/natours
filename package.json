{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning node, express and mongoDB",
  "main": "app.js",
  "targets": {
    "main": false
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:prod": "NODE_ENV=production nodemon server.js",
    "debug": "ndb server.js",
    "delete-data": "NODE_ENV=LOADER node ./dev-data/data/import-dev-data.js --delete",
    "import-data": "NODE_ENV=LOADER node ./dev-data/data/import-dev-data.js --import",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "build:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
    "build": "parcel build ./public/js/index.js --dist-dir ./public/js/bundled",
    "watch": "parcel serve ./public/js/index.js --dist-dir ./public/js/bundled --no-hmr"
  },
  "author": "Saman Fathnazarian",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.3.0",
    "helmet": "^3.23.3",
    "hpp": "^0.2.3",
    "html-to-text": "^8.2.1",
    "i": "^0.3.7",
    "jsonwebtoken": "^8.5.1",
    "mapbox-gl": "^2.7.0",
    "mongoose": "^5.13.14",
    "morgan": "^1.10.0",
    "multer": "^1.4.1",
    "nodemailer": "^6.7.2",
    "npm": "^8.5.5",
    "pug": "^3.0.2",
    "regenerator-runtime": "^0.13.9",
    "sharp": "^0.31.2",
    "slugify": "^1.6.5",
    "stripe": "^7.0.0",
    "validator": "^13.7.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "parcel": "^2.3.2",
    "prettier": "1.17.0",
    "process": "^0.11.10"
  },
  "engines": {
    "node": "16.13.1",
    "npm": "8.19.3"
  }
}
